    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="soma, a" type="Integer" array="False" size=""/>
            <assign variable="soma" expression="0"/>
            <for variable="a" start="25" end="200" direction="inc" step="1">
                <if expression="a%2=0">
                    <then>
                        <assign variable="soma" expression="soma+a"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;A soma dos n&#250;meros pares entre 25 e 200 &#233;: &quot;&amp;soma" newline="True"/>
        </body>
    </function>
